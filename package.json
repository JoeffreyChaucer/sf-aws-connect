{
  "name": "sf-aws-connect",
  "description": "A new CLI generated with oclif",
  "version": "0.1.3",
  "author": "joeffreychaucer",
  "bin": {
    "sf": "./bin/run.js"
  },
  "bugs": "https://github.com/Desktop/sf-aws-connect/issues",
  "dependencies": {
    "@aws-sdk/client-connect": "^3.621.0",
    "@aws-sdk/credential-provider-sso": "^3.621.0",
    "@aws-sdk/types": "^3.609.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "axios": "^1.7.3"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/sinon": "^17.0.3",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "nyc": "^17.0.0",
    "oclif": "^4",
    "shx": "^0.3.3",
    "sinon": "^18.0.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/JoeffreyChaucer/sf-aws-connect",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "sf",
    "dirname": "aws-connect",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "aws-connect": {
        "description": "aws-connect"
      }
    }
  },
  "repository": "JoeffreyChaucer/sf-aws-connect",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --require ts-node/register 'src/commands/**/*.e2e.spec.ts'",
    "version": "oclif readme && git add README.md"
  }
}
